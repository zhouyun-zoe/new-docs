,EAA8B,KAA9B,CAA5F,CAA8I,iBAA9I,CAA+J,kBAAY,UAAU,CAAC,IAAvB,EAA8B,UAA9B,CAA/J,CAAsN,eAAtN,CANF,CAhEK,CAwEL,SAAQ,CACN,KAAM,WADA,CAAR,CAEK,WAFL,CAxEK,CA2EL,SAAQ,CACN,QAAS,EADH,CAAR,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,WAAtB,CAAiC,kBAAY,UAAU,CAAC,IAAvB,EAA8B,OAA9B,CAAjC,CAAqF,KAArF,CAA0F,kBAAY,UAAU,CAAC,IAAvB,EAA8B,SAA9B,CAA1F,CAAgJ,cAAhJ,CAHF,CAIE,UAAI,UAAU,CAAC,IAAf,EAAsB,YAAtB,CAAkC,kBAAY,UAAU,CAAC,IAAvB,EAA8B,WAA9B,CAAlC,CAA0F,MAA1F,CAJF,CAKE,UAAI,UAAU,CAAC,IAAf,EAAoB,kBAAY,UAAU,CAAC,IAAvB,EAA8B,WAA9B,CAApB,CAA4E,eAA5E,CAA2F,kBAAY,UAAU,CAAC,IAAvB,EAA8B,SAA9B,CAA3F,CAAiJ,GAAjJ,CALF,CAME,UAAI,UAAU,CAAC,IAAf,EAAsB,cAAtB,CAAoC,kBAAY,UAAU,CAAC,IAAvB,EAA8B,SAA9B,CAApC,CANF,CA3EK,CAmFL,SAAQ,CACN,KAAM,oBADA,CAAR,CAEK,oBAFL,CAnFK,CAsFL,aAAG,oBAAK,UAAU,CAAC,GAAhB,EAAwB,CACvB,MAAO,oCADgB,CAEvB,MAAO,IAFgB,CAAxB,EAAH,CAtFK,CA0FL,SAAQ,CACN,KAAM,iBADA,CAAR,CAEK,iBAFL,CA1FK,CA6FL,aAAG,oBAAK,UAAU,CAAC,GAAhB,EAAwB,CACvB,MAAO,2CADgB,CAEvB,MAAO,IAFgB,CAAxB,EAAH,CA7FK,CAAP,CAkGD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: 'Architecture'\n};\nexport const metadata = {\n  \"id\": \"advanced/arch\",\n  \"title\": \"Architecture\",\n  \"description\": \"## System Architecture\",\n  \"source\": \"@site/docs/advanced/arch.md\",\n  \"permalink\": \"/new-docs/docs/advanced/arch\",\n  \"editUrl\": \"https://github.com/nervosnetwork/muta/edit/master/website/docs/advanced/arch.md\",\n  \"sidebar\": \"docs\",\n  \"previous\": {\n    \"title\": \"使用 Muta 框架从零开发一条 Dex 专有链\",\n    \"permalink\": \"/new-docs/docs/dev/dex\"\n  },\n  \"next\": {\n    \"title\": \"Transaction, Block and State\",\n    \"permalink\": \"/new-docs/docs/advanced/transaction-block\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: 'System Architecture',\n  id: 'system-architecture',\n  children: []\n}, {\n  value: 'Components',\n  id: 'components',\n  children: []\n}, {\n  value: 'Core Flow',\n  id: 'core-flow',\n  children: [{\n    value: '交易',\n    id: '交易',\n    children: []\n  }, {\n    value: '出块',\n    id: '出块',\n    children: []\n  }]\n}, {\n  value: 'Parallet Execution',\n  id: 'parallet-execution',\n  children: []\n}, {\n  value: 'Execution Block',\n  id: 'execution-block',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h2 {...{\n      \"id\": \"system-architecture\"\n    }}>{`System Architecture`}</h2>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/static/docs-img/arch/arch.png\",\n        \"alt\": null\n      }}></img></p>\n    <h2 {...{\n      \"id\": \"components\"\n    }}>{`Components`}</h2>\n    <p>{`见各组件详细文档`}</p>\n    <h2 {...{\n      \"id\": \"core-flow\"\n    }}>{`Core Flow`}</h2>\n    <blockquote>\n      <p parentName=\"blockquote\">{`用户发 Transaction 到 Transaction 上链发生了什么`}</p>\n    </blockquote>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/static/docs-img/arch/core-flow.png\",\n        \"alt\": null\n      }}></img></p>\n    <h3 {...{\n      \"id\": \"交易\"\n    }}>{`交易`}</h3>\n    <h4 {...{\n      \"id\": \"从-user\"\n    }}>{`从 User`}</h4>\n    <ol>\n      <li parentName=\"ol\">{`用户通过 `}<inlineCode parentName=\"li\">{`SDK`}</inlineCode>{`、`}<inlineCode parentName=\"li\">{`GraphiQL`}</inlineCode>{` 等工具对节点提交一笔交易`}</li>\n      <li parentName=\"ol\"><inlineCode parentName=\"li\">{`Mempool`}</inlineCode>{` 验证交易信息是否合法，典型的有：交易签名、`}<inlineCode parentName=\"li\">{`TimeoutGap`}</inlineCode>{`、`}<inlineCode parentName=\"li\">{`Cycles`}</inlineCode>{` 等，同时存入自身的交易内存池。`}<ul parentName=\"li\">\n          <li parentName=\"ul\">{`交易池达到上限将丢失该交易，同时不会广播。`}</li>\n          <li parentName=\"ul\">{`验证不通过将丢弃该交易，同时不会广播。`}</li>\n        </ul></li>\n      <li parentName=\"ol\">{`当交易验证成功后，调用 `}<inlineCode parentName=\"li\">{`Network`}</inlineCode>{`, `}<inlineCode parentName=\"li\">{`Network`}</inlineCode>{` 把交易序列化后，向已连上的其他节点广播该交易。`}</li>\n    </ol>\n    <h4 {...{\n      \"id\": \"从-network\"\n    }}>{`从 Network`}</h4>\n    <ol {...{\n      \"start\": 4\n    }}>\n      <li parentName=\"ol\">{`本节点通过 `}<inlineCode parentName=\"li\">{`Network`}</inlineCode>{` 收到一笔交易，`}<inlineCode parentName=\"li\">{`Network`}</inlineCode>{` 直接转交给`}<inlineCode parentName=\"li\">{`Mempool`}</inlineCode>{`验证。验证流程参考 `}<inlineCode parentName=\"li\">{`2`}</inlineCode><ul parentName=\"li\">\n          <li parentName=\"ul\">{`不同点: 从 `}<inlineCode parentName=\"li\">{`Network`}</inlineCode>{` 来的交易不会被再次广播`}</li>\n        </ul></li>\n    </ol>\n    <h3 {...{\n      \"id\": \"出块\"\n    }}>{`出块`}</h3>\n    <h4 {...{\n      \"id\": \"提案节点\"\n    }}>{`提案节点`}</h4>\n    <ol {...{\n      \"start\": 5\n    }}>\n      <li parentName=\"ol\"><inlineCode parentName=\"li\">{`Consensus`}</inlineCode>{` 向 `}<inlineCode parentName=\"li\">{`Mempool`}</inlineCode>{` 请求筛选一批交易，`}<inlineCode parentName=\"li\">{`Mempool`}</inlineCode>{` 会根据一些系统参数如: `}<inlineCode parentName=\"li\">{`CyclesLimit`}</inlineCode>{`，来决定最后筛选出的交易数量`}</li>\n      <li parentName=\"ol\"><inlineCode parentName=\"li\">{`Consensus`}</inlineCode>{` 将筛选后的交易打包成 `}<inlineCode parentName=\"li\">{`Block`}</inlineCode>{` 并且对它签名后组成 `}<inlineCode parentName=\"li\">{`Proposal`}</inlineCode>{`，然后通过 `}<inlineCode parentName=\"li\">{`Network`}</inlineCode>{` 广播给其他节点。`}<ul parentName=\"li\">\n          <li parentName=\"ul\"><inlineCode parentName=\"li\">{`Muta`}</inlineCode>{` 使用了 `}<inlineCode parentName=\"li\">{`Compact Blocks`}</inlineCode>{`，所以 `}<inlineCode parentName=\"li\">{`Block`}</inlineCode>{` 中并不存放交易的具体内容`}</li>\n        </ul></li>\n    </ol>\n    <h4 {...{\n      \"id\": \"提案块投票\"\n    }}>{`提案块投票`}</h4>\n    <ol {...{\n      \"start\": 7\n    }}>\n      <li parentName=\"ol\">{`通过 `}<inlineCode parentName=\"li\">{`Network`}</inlineCode>{` 收到提案节点的`}<inlineCode parentName=\"li\">{`Proposal`}</inlineCode>{`，验证 `}<inlineCode parentName=\"li\">{`Proposal`}</inlineCode>{` 的签名、区块参数等基础信息。`}</li>\n      <li parentName=\"ol\">{`查看本地是否存在对应交易。`}</li>\n      <li parentName=\"ol\">{`不存在的交易将通过 `}<inlineCode parentName=\"li\">{`Network`}</inlineCode>{` 从网络获取。`}</li>\n      <li parentName=\"ol\">{`验证通过后将对 `}<inlineCode parentName=\"li\">{`Proposal`}</inlineCode>{` 投 `}<inlineCode parentName=\"li\">{`Yes`}</inlineCode>{` 票，详细的共识流程可以查看 `}<inlineCode parentName=\"li\">{`Overlord`}</inlineCode>{` 设计文档，这里不再赘述。`}</li>\n    </ol>\n    <h4 {...{\n      \"id\": \"commit-区块\"\n    }}>{`Commit 区块`}</h4>\n    <ol {...{\n      \"start\": 11\n    }}>\n      <li parentName=\"ol\">{`存储共识成功后的 `}<inlineCode parentName=\"li\">{`Block`}</inlineCode>{` 到 `}<inlineCode parentName=\"li\">{`Storage`}</inlineCode>{`, 同时进入下一轮共识。`}</li>\n      <li parentName=\"ol\">{`异步的将交易提交给 `}<inlineCode parentName=\"li\">{`Framework`}</inlineCode>{` 执行。`}</li>\n      <li parentName=\"ol\"><inlineCode parentName=\"li\">{`Framework`}</inlineCode>{` 根据交易内容找到对应的 `}<inlineCode parentName=\"li\">{`Service`}</inlineCode>{`。`}</li>\n      <li parentName=\"ol\">{`执行成功后，数据存储到 `}<inlineCode parentName=\"li\">{`Storage`}</inlineCode></li>\n    </ol>\n    <h2 {...{\n      \"id\": \"parallet-execution\"\n    }}>{`Parallet Execution`}</h2>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/static/docs-img/arch/parallet.png\",\n        \"alt\": null\n      }}></img></p>\n    <h2 {...{\n      \"id\": \"execution-block\"\n    }}>{`Execution Block`}</h2>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/static/docs-img/arch/execution-block.png\",\n        \"alt\": null\n      }}></img></p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}